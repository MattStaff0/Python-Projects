# Matthew Stafford 
from random import*
answer = randint(1, 100)
guess = int(input("Guess the random number from 1-100: "))
tries = 1
while guess != answer:
    tries = tries + 1
    print("Try again!")
    guess = int(input("Guess another number:"))
    if guess <= answer:
        print("Pick a HIGHER number")
    if guess >= answer:
        print("Pick a LOWER number")
    
print("Congrats!")
print("You won in", tries,"tries!")



    
